#include "libft.h"

void *ft_memcpy(void *dest, const void *src, size_t n)
{
  	size_t i;

    i = 0;
    char *c_src = (char *)src;
    char *c_dest = (char *)dest;
    while (i < n)
      c_dest[i++] = c_src[i];
    c_dest[i] = '\0';
		return (c_dest);
}

